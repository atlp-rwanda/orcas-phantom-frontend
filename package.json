{
  "name": "orcas-phantom-frontend",
  "version": "1.0.0",
  "description": "Phantom is a web application that allows passengers to track bus location and movements around Kigali",
  "main": "index.js",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/webpack/test.js"
    ]
  },
  "scripts": {
    "start": "node ./server.js",
    "dev": "webpack-dev-server --mode development --config webpack.config.js --open --fix",
    "build": "webpack --mode production --config webpack.config.js",
    "test": "jest --verbose ./src/__test__/",
    "test:watch": "jest --watch --verbose ./src/__test__/",
    "test:coverage": "jest --coverage --colors --verbose ./src/__test__/",
    "lint": "eslint --ext .jsx --ext  .js . --fix",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/orcas-phantom-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/orcas-phantom-frontend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/orcas-phantom-frontend#readme",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "antd": "^4.7.0",
    "axios": "^0.21.0",
    "chart.js": "^2.9.4",
    "express": "^4.17.1",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "leaflet": "^1.7.1",
    "node-gyp": "^7.1.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.10.0",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-leaflet": "^3.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-spring": "^8.0.27",
    "react-toastify": "^6.0.9",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.2",
    "chokidar": "^3.4.2",
    "coveralls": "^3.1.0",
    "css-loader": "^4.3.0",
    "dotenv-webpack": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-react": "^7.21.0",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-emotion": "^10.0.32",
    "jest-styled-components": "^7.0.3",
    "mini-css-extract-plugin": "^0.11.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "react-bootstrap": "^1.3.0",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^1.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
